Provider.js",[],"D:\\Latest react\\Http and forms to food order app\\src\\components\\Layout\\Header.js",[],"D:\\Latest react\\Http and forms to food order app\\src\\components\\Meals\\Meals.js",[],"D:\\Latest react\\Http and forms to food order app\\src\\components\\Cart\\Cart.js",["78"],"import { useContext, useState } from \"react\";\n\nimport Modal from \"../UI/Modal\";\nimport CartItem from \"./CartItem\";\nimport classes from \"./Cart.module.css\";\nimport CartContext from \"../../store/cart-context\";\nimport Checkout from \"./Checkout\";\n\nconst Cart = (props) => {\n  const [isCheckout, setIsCheckout] = useState(false);\n  const cartCtx = useContext(CartContext);\n\n  const totalAmount = `$${cartCtx.totalAmount.toFixed(2)}`;\n  const hasItems = cartCtx.items.length > 0;\n\n  const checkOutHandler = (event) => {\n    setIsCheckout(true);\n  };\n\n  const cartItemRemoveHandler = (id) => {\n    cartCtx.removeItem(id);\n  };\n\n  const cartItemAddHandler = (item) => {\n    cartCtx.addItem(item);\n  };\n\n  const submitHandler = async (userData) => {\n    const response = await fetch(\n      `https://react-meals-data-default-rtdb.firebaseio.com/orders.json`,\n      {\n        method: \"POST\",\n        body: JSON.stringify({\n          user: userData,\n          orderedItems: cartCtx.items,\n        }),\n      }\n    );\n  };\n\n  const cartItems = (\n    <ul className={classes[\"cart-items\"]}>\n      {cartCtx.items.map((item) => (\n        <CartItem\n          key={item.id}\n          name={item.name}\n          amount={item.amount}\n          price={item.price}\n          onRemove={cartItemRemoveHandler.bind(null, item.id)}\n          onAdd={cartItemAddHandler.bind(null, item)}\n        />\n      ))}\n    </ul>\n  );\n\n  return (\n    <Modal onClose={props.onClose}>\n      {cartItems}\n      <div className={classes.total}>\n        <span>Total Amount</span>\n        <span>{totalAmount}</span>\n      </div>\n      {isCheckout && (\n        <Checkout onConfirm={submitHandler} onCancel={props.onClose}></Checkout>\n      )}\n      {!isCheckout && (\n        <div className={classes.actions}>\n          <button className={classes[\"button--alt\"]} onClick={props.onClose}>\n            Close\n          </button>\n          {hasItems && (\n            <button className={classes.button} onClick={checkOutHandler}>\n              Order\n            </button>\n          )}\n        </div>\n      )}\n    </Modal>\n  );\n};\n\nexport default Cart;\n","D:\\Latest react\\Http and forms to food order app\\src\\store\\cart-context.js",[],"D:\\Latest react\\Http and forms to food order app\\src\\components\\Layout\\HeaderCartButton.js",[],"D:\\Latest react\\Http and forms to food order app\\src\\components\\Meals\\MealsSummary.js",[],"D:\\Latest react\\Http and forms to food order app\\src\\components\\Cart\\CartItem.js",[],"D:\\Latest react\\Http and forms to food order app\\src\\components\\Meals\\AvailableMeals.js",[],"D:\\Latest react\\Http and forms to food order app\\src\\components\\Cart\\Checkout.js",[],"D:\\Latest react\\Http and forms to food order app\\src\\components\\UI\\Modal.js",[],"D:\\Latest react\\Http and forms to food order app\\src\\components\\Cart\\CartIcon.js",[],"D:\\Latest react\\Http and forms to food order app\\src\\components\\UI\\Card.js",[],"D:\\Latest react\\Http and forms to food order app\\src\\components\\Meals\\MealItem\\MealItem.js",[],"D:\\Latest react\\Http and forms to food order app\\src\\components\\Meals\\MealItem\\MealItemForm.js",[],"D:\\Latest react\\Http and forms to food order app\\src\\components\\UI\\Input.js",[],{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","replacedBy":"82"},{"ruleId":"83","severity":1,"message":"84","line":29,"column":11,"nodeType":"85","messageId":"86","endLine":29,"endColumn":19},"no-native-reassign",["87"],"no-negated-in-lhs",["88"],"no-unused-vars","'response' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]